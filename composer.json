{
    "name": "captjm/storyteller-bundle",
    "description": "Storyteller",
    "type": "symfony-bundle",
    "autoload": {
        "psr-4": {
            "CaptJM\\Bundle\\StorytellerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CaptJM\\Bundle\\StorytellerBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "David Makaridze",
            "email": "dmakaridze@gmail.com"
        }
    ],
    "license": [
        "MIT"
    ],
    "require": {
        "doctrine/orm": "^2.14",
        "symfony/framework-bundle": "^6.2",
        "doctrine/doctrine-bundle": "^2.9",
        "easycorp/easyadmin-bundle": "^4.6",
        "friendsofsymfony/ckeditor-bundle": "^2.4",
        "helios-ag/fm-elfinder-bundle": "^12.2"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "extra": {
        "symfony": {
            "endpoint": [
                "https://api.github.com/repos/captjm/storyteller/contents/index.json",
                "flex://defaults"
            ]
        }
    }
}
